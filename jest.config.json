{
  "coveragePathIgnorePatterns": ["node_modules", "proto"],
  "preset": "ts-jest",
  "moduleNameMapper": {
    "@proto/(.*)": "<rootDir>/proto/$1",
    "@root/(.*)": "<rootDir>/$1"
  },
  "modulePathIgnorePatterns": [
    "__tests__/data_structures.ts",
    "__tests__/helpers.ts",
    "__tests__/mocks"
  ]
}
